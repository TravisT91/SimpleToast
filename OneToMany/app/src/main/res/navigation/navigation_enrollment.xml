<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation_enrollment"
    app:startDestination="@id/getStartedFragment">

    <fragment
        android:id="@+id/cardActiveFragment"
        android:name="com.engageft.fis.pscu.feature.CardActiveFragment"
        android:label="CardActiveFragment" >
        <action
            android:id="@+id/action_cardActiveFragment_to_authenticatedActivity2"
            app:destination="@id/authenticatedActivity2" />
    </fragment>
    <fragment
        android:id="@+id/cardLinkedFragment"
        android:name="com.engageft.fis.pscu.feature.CardLinkedFragment"
        android:label="CardLinkedFragment" >
        <action
            android:id="@+id/action_cardLinkedFragment_to_authenticatedActivity2"
            app:destination="@id/authenticatedActivity2" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragment"
        android:name="com.engageft.fis.pscu.feature.CreateAccountFragment"
        android:label="CreateAccountFragment" >
        <action
            android:id="@+id/action_createAccountFragment_to_verifyIdentityFragment"
            app:destination="@id/verifyIdentityFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_createAccountFragment_to_termsOfUseFragment"
            app:destination="@id/termsOfUseFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_createAccountFragment_to_sendingEnrollmentFragment"
            app:destination="@id/sendingEnrollmentFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/enrollmentCardPinFragment"
        android:name="com.engageft.fis.pscu.feature.EnrollmentCardPinFragment"
        android:label="EnrollmentCardPinFragment" >
        <action
            android:id="@+id/action_enrollmentCardPinFragment_to_createAccountFragment"
            app:destination="@id/createAccountFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_enrollmentCardPinFragment_to_verifyIdentityFragment"
            app:destination="@id/verifyIdentityFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_enrollmentCardPinFragment_to_termsOfUseFragment"
            app:destination="@id/termsOfUseFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_enrollmentCardPinFragment_to_sendingEnrollmentFragment"
            app:destination="@id/sendingEnrollmentFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/enrollmentErrorFragment"
        android:name="com.engageft.fis.pscu.feature.EnrollmentErrorFragment"
        android:label="EnrollmentErrorFragment" />
    <fragment
        android:id="@+id/enrollmentSuccessFragment"
        android:name="com.engageft.fis.pscu.feature.EnrollmentSuccessFragment"
        android:label="EnrollmentSuccessFragment" >
        <action
            android:id="@+id/action_enrollmentSuccessFragment_to_cardActiveFragment"
            app:destination="@id/cardActiveFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_enrollmentSuccessFragment_to_cardLinkedFragment"
            app:destination="@id/cardLinkedFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/getStartedFragment"
        android:name="com.engageft.fis.pscu.feature.GetStartedFragment"
        android:label="GetStartedFragment" >
        <action
            android:id="@+id/action_getStartedFragment_to_enrollmentCardPinFragment"
            app:destination="@id/enrollmentCardPinFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_getStartedFragment_to_createAccountFragment"
            app:destination="@id/createAccountFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_getStartedFragment_to_verifyIdentityFragment"
            app:destination="@id/verifyIdentityFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_getStartedFragment_to_termsOfUseFragment"
            app:destination="@id/termsOfUseFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_getStartedFragment_to_sendingEnrollmentFragment"
            app:destination="@id/sendingEnrollmentFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <argument
            android:name="@string/navigation_override_up"
            android:defaultValue="true" />
    </fragment>
    <fragment
        android:id="@+id/sendingEnrollmentFragment"
        android:name="com.engageft.fis.pscu.feature.SendingEnrollmentFragment"
        android:label="SendingEnrollmentFragment" >
        <action
            android:id="@+id/action_sendingEnrollmentFragment_to_enrollmentSuccessFragment"
            app:destination="@id/enrollmentSuccessFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_sendingEnrollmentFragment_to_enrollmentErrorFragment"
            app:destination="@id/enrollmentErrorFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/termsOfUseFragment"
        android:name="com.engageft.fis.pscu.feature.TermsOfUseFragment"
        android:label="TermsOfUseFragment" >
        <action
            android:id="@+id/action_termsOfUseFragment_to_sendingEnrollmentFragment"
            app:destination="@id/sendingEnrollmentFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/verifyIdentityFragment"
        android:name="com.engageft.fis.pscu.feature.VerifyIdentityFragment"
        android:label="VerifyIdentityFragment" >
        <action
            android:id="@+id/action_verifyIdentityFragment_to_termsOfUseFragment"
            app:destination="@id/termsOfUseFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
        <action
            android:id="@+id/action_verifyIdentityFragment_to_sendingEnrollmentFragment"
            app:destination="@id/sendingEnrollmentFragment"
            app:enterAnim="@anim/nav_enter_anim"
            app:exitAnim="@anim/nav_exit_anim"
            app:popEnterAnim="@anim/nav_pop_enter_anim"
            app:popExitAnim="@anim/nav_pop_exit_anim" />
    </fragment>
    <activity
        android:id="@+id/authenticatedActivity2"
        android:name="com.engageft.fis.pscu.AuthenticatedActivity"
        android:label="AuthenticatedActivity" />
</navigation>